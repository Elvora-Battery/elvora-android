    </style>

    <style name="TextAppearance.Elvora.HeadlineMediumSemiBold" parent="TextAppearance.Material3.HeadlineMedium">
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
    </style>

    <style name="TextAppearance.Elvora.BodyLarge" parent="TextAppearance.Material3.BodyLarge">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="TextAppearance.Elvora.HeadlineSmallSemiBold" parent="TextAppearance.Material3.HeadlineSmall">
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
    </style>

    <style name="TextAppearance.Elvora.DisplayMedium" parent="TextAppearance.Material3.DisplayMedium">
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
    </style>

    <style name="Widget.Elvora.TextInputLayout" parent="Widget.Material3.TextInputLayout.OutlinedBox">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.TextInputLayout</item>
        <item name="shapeAppearance">@style/ShapeAppearance.Elvora.SmallComponent</item>
        <item name="hintTextColor">?attr/colorPrimary</item>
    </style>

    <style name="ThemeOverlay.App.TextInputLayout" parent="">
        <item name="colorPrimary">@color/md_theme_primary</item>
        <item name="colorOnSurface">?attr/colorOnSurface</item>
        <item name="textAppearanceTitleMedium">@style/TextAppearance.Elvora.TitleMedium</item>
        <item name="textAppearanceBodySmall">@style/TextAppearance.Elvora.BodySmall</item>
    </style>
