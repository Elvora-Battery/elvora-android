        <item name="colorSurfaceContainerLow">@color/md_theme_surfaceContainerLow</item>
        <item name="colorSurfaceContainer">@color/md_theme_surfaceContainer</item>
        <item name="colorSurfaceContainerHigh">@color/md_theme_surfaceContainerHigh</item>
        <item name="colorSurfaceContainerHighest">@color/md_theme_surfaceContainerHighest</item>
    </style>

    <style name="ShapeAppearanceOverlay.Elvora.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearance.Elvora.SmallComponent" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">16dp</item>
    </style>

    <style name="ShapeAppearance.Elvora.SmallComponent2" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">cut</item>
        <item name="cornerSize">4dp</item>
    </style>

    <style name="ShapeAppearance.Elvora.SmallComponent50Percent" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">100dp</item>
    </style>

    <style name="ShapeAppearance.Elvora.MediumComponent1" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <style name="ShapeAppearance.Elvora.MediumComponent2" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <style name="ShapeAppearance.Elvora.MediumComponent3" parent="ShapeAppearance.Material3.MediumComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
    </style>

    <style name="TextAppearance.Elvora.BodyMedium" parent="TextAppearance.Material3.BodyMedium">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="TextAppearance.Elvora.BodySmall" parent="TextAppearance.Material3.BodySmall">
        <item name="fontFamily">@font/poppins_regular</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
    </style>

    <style name="TextAppearance.Elvora.BodySmallSemiBold" parent="TextAppearance.Material3.BodySmall">
        <item name="fontFamily">@font/poppins_semi_bold</item>
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
    </style>

    <style name="TextAppearance.Elvora.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="fontFamily">@font/poppins_medium</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
    </style>

    <style name="TextAppearance.Elvora.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="fontFamily">@font/poppins_medium</item>
